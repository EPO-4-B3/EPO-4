function res = setSteering(angle_deg)
   
    if(angle_deg == 0)
        val = 150; 
    elseif( angle > 0 && angle < 25.7831) % maximum steering radius is 25.7 degrees = 0.45 rad
        val = 150+angle_deg*;
    elseif( angle < 0 && angle > -25.7831)
        val = 150 + force; 
    else 
        res = 0;
        return;
    end 
    str = sprintf("M%d",val);
    res = str;    
    EPOCommunications('transmit',str); 
    
end 
    